{
  "name": "algo-trade-service",
  "version": "2.0.0",
  "description": "Modern TypeScript backend for managing algorithmic trading data",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "node --test"
